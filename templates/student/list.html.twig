<h1>
	Student index
</h1>
{{ dump(students) }}
<table border="2">
	<thead>
		<tr>
			<th>
				Nsc
			</th>
			<th>
				Email
			</th>
			<th>
				actions
			</th>
		</tr>
	</thead>
	<tbody>
		{% for student in students %}
			<tr>
				<td>
					{{ student.nsc }}
				</td>
				<td>
					{{ student.email }}
				</td>

			</tr>
		{% endfor %}
	</tbody>
</table>
